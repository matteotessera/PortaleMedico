<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="190.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dashapp.controller.dashboardPatient.AddSintomoController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="20.0" layoutY="9.0" prefHeight="170.0" prefWidth="359.0" spacing="10.0">
         <children>
            <Label text="REGISTRA SINTOMO">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <HBox alignment="TOP_CENTER" prefHeight="42.0" prefWidth="450.0" spacing="10.0">
               <children>
                  <DatePicker fx:id="dataField" promptText="Data Inizio" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="oraField" promptText="Orario" />
               </children>
            </HBox>
            <TextField fx:id="descrizioneField" prefHeight="26.0" prefWidth="392.0" promptText="Descrivi il sintomo" />
            <Button mnemonicParsing="false" onAction="#registraSintomo" text="Registra Sintomo" />
         </children>
      </VBox>
   </children>
</AnchorPane>
