<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="934.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dashapp.controller.dashboardPatient.fascicolo.FascicoloPazienteController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="934.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label fx:id="Hpatologie" alignment="CENTER" onMouseClicked="#showPatologie" prefHeight="50.0" prefWidth="1000.0" text="Patologie" textFill="#ad343e" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="Hterapie" alignment="CENTER" onMouseClicked="#showTerapie" prefHeight="50.0" prefWidth="1000.0" text="Terapie" textFill="#f2af29" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="Hsintomi" alignment="CENTER" onMouseClicked="#showSintomi" prefHeight="50.0" prefWidth="1000.0" text="Sintomi" textFill="#9caf39" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <StackPane fx:id="contentPane" prefHeight="514.0" prefWidth="934.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
